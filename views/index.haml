%section
  %form.search{ action: "/whois/", method: "get" }
    %input{ type: "text", name: "name" }
    %input{ type: "submit", value: "Search" }
  

%ul#results  
  /
    - unless @name.nil?
      - @results.each do |whois|
        = whois.domain
        is
        = whois.available? ? "available" : "registered"
      
%p
  = @results[0]
  is
  = @results ? 'registered' : 'available'
    

